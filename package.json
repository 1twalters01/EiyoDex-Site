{
    "name": "EiyoDex-Site",
    "private": true,
    "version": "1.0.0",
    "packageManager": "pnpm@9.6.0",
    "workspaces": [
        "apps/astro/*"
    ],
    "scripts": {
        "test": "pnpm -r run test",
        "dev": "pnpm -r run dev",
        "build": "pnpm -r run build",
        "preview": "pnpm -r run preview",
        "lint": "pnpm -r run lint",
        "format": "prettier --config .prettierrc.toml \"**/*.{ts,scss}\" --write"
    },
    "dependencies": {
        "dotenv": "^17.2.1"
    },
    "devDependencies": {
        "prettier": "3.6.2",
        "sass": "^1.90.0",
        "typescript": "^5.8.3"
    }
}
